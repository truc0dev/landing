{
  "version": 2,
  "builds": [
    {
      "src": "landing-angular/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "landing-angular/dist/landing"
      }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
