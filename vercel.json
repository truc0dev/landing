{
  "version": 2,
  "builds": [
    {
      "src": "landing-angular/package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run vercel-build",
        "outputDirectory": "landing-angular/dist/landing-angular/browser/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 